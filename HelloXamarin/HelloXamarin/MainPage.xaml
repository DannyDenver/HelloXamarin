<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns:local="clr-namespace:HelloXamarin"
  xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="HelloXamarin.MainPage">
  <ContentPage Title="Map" Icon="clock.png">
    <Label Text="Map" HorizontalOptions="Center" VerticalOptions="Center" />
  </ContentPage>
  
  <NavigationPage Title="Properties" Icon="clock.png">
    <x:Arguments>
      <local:ListPage />
    </x:Arguments>   
  </NavigationPage> 

  <ContentPage Title="Settings" Icon="clock.png">
    <Label Text="Content of Page2" HorizontalOptions="Center" VerticalOptions="Center" />
  </ContentPage> 
  
</TabbedPage>